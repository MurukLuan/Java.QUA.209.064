package principal;

public class Program {

	public static void main(String[] args) {
		
		for(int contador = 1; contador <= 20; contador++) {
			System.out.println(contador);
		}
		
		for(int contador = 1; contador <= 20; contador++) {
			System.out.print(" " + contador );
		}
		
		int contador = 1;
		while(contador <= 20) {
			contador = contador + 1;
			System.out.println(contador);
			
		}
		

	}

}
